#################################################################
#    Object Properties
#################################################################

###  http://ex.org/age
:age rdf:type owl:ObjectProperty ;
     rdfs:subPropertyOf owl:topObjectProperty ;
     rdfs:range :Age .


###  http://ex.org/created_in
:created_in rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:range :Building .


###  http://ex.org/expansion
:expansion rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:range :Expansion .


###  http://ex.org/upgraded_from
:upgraded_from rdf:type owl:ObjectProperty ;
               owl:equivalentProperty [ owl:inverseOf :upgrades_to
                                      ] ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:inverseOf :upgrades_to .


###  http://ex.org/upgrades_to
:upgrades_to rdf:type owl:ObjectProperty ;
             owl:equivalentProperty [ owl:inverseOf :upgraded_from
                                    ] ;
             rdfs:subPropertyOf owl:topObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://ex.org/Age
:Age rdf:type owl:Class ;
     rdfs:subClassOf owl:Thing ;
     owl:disjointWith owl:Nothing .


###  http://ex.org/Animal
:Animal rdf:type owl:Class ;
        rdfs:subClassOf owl:Thing ;
        owl:disjointWith owl:Nothing .


###  http://ex.org/Building
:Building rdf:type owl:Class ;
          rdfs:subClassOf owl:Thing ;
          owl:disjointWith owl:Nothing .


###  http://ex.org/Expansion
:Expansion rdf:type owl:Class ;
           rdfs:subClassOf owl:Thing ;
           owl:disjointWith owl:Nothing .


###  http://ex.org/Unit
:Unit rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      owl:disjointWith owl:Nothing .


###  http://www.w3.org/2002/07/owl#Nothing
owl:Nothing rdfs:subClassOf owl:Nothing ;
            owl:disjointWith owl:Thing .


